VITE_API_BASE_URL=http://localhost:8000

# Set to 1 to enable provider avatar API calls from Live Interview UI.
VITE_ENABLE_PROVIDER_AVATAR=1

# Simli remote room audio:
# 1 = on (recommended for avatar-primary voice)
# 0 = off (only for debugging)
VITE_SIMLI_PLAY_REMOTE_AUDIO=1

# Barge-in tuning (optional)
VITE_BARGE_IN_SAMPLE_MS=120
VITE_BARGE_IN_RMS_THRESHOLD=0.05
VITE_BARGE_IN_CONSECUTIVE_FRAMES=7
VITE_BARGE_IN_DYNAMIC_MULTIPLIER=2.25
